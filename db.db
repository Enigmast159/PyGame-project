CREATE TABLE Sets (
    Name     STRING  PRIMARY KEY
                     UNIQUE
                     NOT NULL,
    Cost     INTEGER NOT NULL,
    Is_buyed BOOLEAN DEFAULT (0) 
);
CREATE TABLE Statistics (
    Num         INTEGER PRIMARY KEY AUTOINCREMENT,
    Points      INTEGER DEFAULT (0) 
                        NOT NULL,
    [Max coins] INTEGER DEFAULT (0) 
                        NOT NULL
);
